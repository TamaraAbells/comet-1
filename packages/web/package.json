{
  "name": "web",
  "version": "0.0.1",
  "main": "main/index.js",
  "scripts": {
    "dev": "concurrently \"SET BROWSER=none && yarn dev:vite\" \"yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "yarn run build:electron && electron .",
    "build": "yarn run build:vite && yarn run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "yarn run build && electron-builder",
    "pack": "yarn run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@headlessui/react": "^0.2.0",
    "@popperjs/core": "^2.6.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "@tippyjs/react": "^4.2.0",
    "core": "0.0.1",
    "electron-context-menu": "^2.4.0",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^7.0.0",
    "framer-motion": "^3.2.1",
    "fuse.js": "^6.4.6",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "react": "^17.0.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "react-dom": "^17.0.0",
    "react-hook-form": "^6.14.2",
    "react-hot-toast": "^1.0.1",
    "react-icons": "^4.1.0",
    "react-intersection-observer": "^8.31.0",
    "react-player": "^2.8.1",
    "react-popper": "^2.2.4",
    "react-query": "^3.6.0",
    "react-responsive-modal": "^6.0.1",
    "react-router-dom": "^5.2.0",
    "react-scrollbars-custom": "^4.0.25",
    "react-twemoji": "^0.3.0",
    "react-twitter-widgets": "^1.9.5",
    "react-use": "^15.3.8",
    "react-virtualized-auto-sizer": "^1.0.4",
    "react-virtuoso": "^1.4.0",
    "react-window": "^1.8.6",
    "remirror": "~1.0.0-next.60",
    "subscriptions-transport-ws": "^0.9.18",
    "twemoji": "^13.0.1",
    "zustand": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "autoprefixer": "^10.2.3",
    "concurrently": "^5.3.0",
    "electron": "^11.2.0",
    "electron-builder": "^22.9.1",
    "postcss": "^8.2.4",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.2",
    "vite": "^2.0.0-beta.50"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
